<!--See Unicorn.config for commentary on how configurations operate, or https://github.com/kamsar/Unicorn/blob/master/README.md-->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<unicorn>
			<configurations>
				<configuration name="Feature.Product" description=" Feature Product" dependencies="Foundation.*" >
					<predicate>
						<include name="Templates" database="master" path="/sitecore/templates/Feature/Product"/>
						<include name="Renderings" database="master" path="/sitecore/layout/Renderings/Feature/Product"/>
					</predicate>
				</configuration>
			</configurations>
		</unicorn>
	</sitecore>
</configuration>
